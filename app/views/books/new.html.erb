<div class="container">
  <div class="low">
    <%= form_for @book do |f| %>
        <%= f.label :本のタイトル %>
        <%= f.text_field :name %>
          <%= fields_for @author do |author| %>
            <%= author.label :著者名 %>
            <%= author.text_field :name %>
          <% end %>
      <%= f.submit "投稿"%>
    <% end %>
  </div>
</div>

<h1>Books#new</h1>
<p>Find me in app/views/books/new.html.erb</p>

<div class="low">
    <div class="col-xs-9 col-xs-offset-1">
    <h2>新規登録</h2>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>
    <table class="table table-bordered">
      <tr>
        <th class="info">ユーザー名</th>
        <th>
          <div class="field">
            <%= f.text_field :name,
                :placeholder => "2~20文字以内で記入してください",
                autofocus: true, autocomplete: "name",:size => "50*30"
            %>
          </div>
        </th>
      </tr>
      <tr>
        <th class="info">メールアドレス</th>
        <th>
          <div class="field">
            <%= f.email_field :email, autofocus: true, autocomplete: "email",:size => "50*30" %>
          </div>
        </th>
      </tr>

      <tr>
        <th class="info">パスワード</th>
        <th>
          <div class="field">
            <%= f.password_field :password,
                :placeholder => "6文字以上の記入を御願いします",
                autocomplete: "new-password",:size => "50*30"
            %>
          </div>
        </th>
      </tr>

      <tr>
        <th class="info">パスワード確認</th>
        <th>
          <div class="field">
            <%= f.password_field :password_confirmation, autocomplete: "new-password",:size => "50*30" %>
          </div>
        </th>
      </tr>
    </table>
    <div class="row">
      <div class="actions col-xs-2 col-xs-offset-5">
      <%= f.submit "新規登録",class:"btn btn-primary" %>
    </div>
    <% end %>
  </div>
  </div>
</div>

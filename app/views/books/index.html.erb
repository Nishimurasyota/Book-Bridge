<div class="container">
  <div class="low">
    <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>本のタイトル</th>
          <th>著者名</th>
        </tr>
      </thead>
      <% @books.each do |f| %>
      <tbody>
        <tr>
          <td>
            <%= link_to (f) do %>
              <%= f.name %>
            <% end %>
          </td>
          <td>
            <%= f.author.name %>
          </td>
        </tr>
      </tbody>
      <% end %>
    </table>
  </div>
</div>

<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>
